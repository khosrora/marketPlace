<%- include("./includeBreadCrumb")%>
    <!-- my account start  -->
    <section class="main_content_area">
        <div class="container">
            <div class="account_dashboard">
                <div class="row">
                    <div class="col-sm-12 col-md-3 col-lg-3">
                        <!-- Nav tabs -->
                        <%- include("./includeMenu")%>
                    </div>
                    <div class="col-sm-12 col-md-9 col-lg-9">
                        <!-- Tab panes -->
                        <div class="tab-content dashboard_content">
                            <div class="tab-pane fade show active" id="dashboard">
                                <p>نام خریدار : <%= order.user.firstName %>
                                        <%= order.user.lastName %>
                                </p>
                                <p>شماره همراه خریدار : <%= order.user.mobile %>
                                </p>
                                <p>پست الکترونیک خریدار : <%= order.user.email %>
                                </p>
                                <p>آدرس خریدار : <%= order.user.address %>
                                </p>
                                <p>محصول خریداری شده : <%= order.titleProduct %>
                                </p>
                                <p>تعداد خریداری شده : <%= order.quantityProduct %>
                                </p>
                                <p>مبلغ محصول خریداری شده : <%= order.priceProduct %>
                                </p>
                                <p>تاریخ سفارش : <%= jalaliMoment(order.createdAt) %>
                                </p>
                                <p>
                                    <% if(!order.isSend){%>
                                        <a href="/seller/isSend/<%= order.uniqueCode  %>"><span class="badge badge-danger p-3">ارسال شده</span></a>
                                        <%}%>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- my account end   -->