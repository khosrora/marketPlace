<div class="shop_toolbar t_bottom">
    <div class="pagination">
        <ul>
            <% if(currentPage !==1 && previousPage !==1 ) {%>
                <li class="current"><a href="?page=1">1</a></li>
                <%}%>
                    <%if(hasPeriviousPage){%>
                        <li class=""><a href="?page=<%= previousPage %>">
                                <%= previousPage %>
                            </a></li>
                        <%}%>
                            <li><a href="?page=<%=currentPage%>">
                                    <%=currentPage%>
                                </a></li>

                            <%if(hasNextPage){%>
                                <li><a href="?page=<%= nextPage %>">
                                        <%= nextPage %>
                                    </a></li>
                                <%}%>
                                    <% if(lastPage !==currentPage && nextPage!==lastPage) {%>
                                        <li><a href="?page=<%= lastPage %>">
                                                <%= lastPage %>
                                            </a></li>
                                        <%}%>
        </ul>
    </div>
</div>