<!--product details start-->
<div class="product_details mt-60 mb-60">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6">
                <div class="product-details-tab">
                    <div id="img-1" class="zoomWrapper single-zoom">
                        <a href="#">
                            <img id="zoom1" id="image" src="/uploads/images/product/<%= product.image[0] %>"
                                data-zoom-image="/uploads/images/product/<%= product.image[0] %>" alt="<%= product.title %>">
                        </a>
                    </div>
                    <div class="single-zoom-thumb">
                        <ul class="s-tab-zoom owl-carousel single-product-active" id="gallery_01">
                            <% for(item of product.image){%>
                                <li>
                                    <a href="#" class="elevatezoom-gallery active" data-update=""
                                        data-image="/uploads/images/product/<%= item %>"
                                        data-zoom-image="/uploads/images/product/<%= item %>">
                                        <img src="/uploads/images/product/<%= item %>" alt="zo-th-1">
                                    </a>
                                </li>
                                <%}%>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="product_d_right">
                    <form action="#" id="card__item">
                        <input type="hidden" id="product__id" value="<%= product.code %>">
                        <input type="hidden" id="product__id_Seller" value="<%= product.seller %>">
                        <input type="hidden" id="product__image" value="<%= product.image[0] %>">
                        <h1 id="product__title">
                            <%= product.title %>
                        </h1>
                        <div class="price_box">
                            <span class="current_price">
                                <%= product.price %>
                            </span>
                        </div>
                        <div class="product_desc">
                            <p>
                                <%= truncate(product.desc , 400) %>
                            </p>
                        </div>
                        <div class="product_variant quantity">
                            <button class="button" type="submit" id="addTocart">افزودن به سبد</button>
                        </div>
                        <div class=" product_d_action">
                            <ul>
                                <li><a href="#" title="افزودن به علاقه‌مندی‌ها">+ افزودن به علاقه‌مندی‌ها</a></li>
                            </ul>
                        </div>
                        <div class="product_meta">
                            <span>دسته: <a href="#">
                                    <%= category.name %>
                                </a></span>
                        </div>
                    </form>
                    <div class="priduct_social">
                        <ul>
                            <% for(item of category.categoryItem){%>
                                <li><a class="facebook" href="#" title="<%= item.name %>"><i class="fa fa-tag"></i>
                                        <%= item.name %>
                                    </a></li>
                                <%}%>
                        </ul>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<!--product details end-->

<!--product info start-->
<div class="product_d_info mb-60">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="product_d_inner">
                    <div class="product_info_button">
                        <ul class="nav" role="tablist">
                            <li>
                                <a class="active" data-toggle="tab" href="#info" role="tab" aria-controls="info"
                                    aria-selected="false">توضیحات</a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#sheet" role="tab" aria-controls="sheet"
                                    aria-selected="false">مشخصات فنی</a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#reviews" role="tab" aria-controls="reviews"
                                    aria-selected="false">نقد و برررسی (<%= comments.length %>)</a>
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <div class="tab-pane fade show active" id="info" role="tabpanel">
                            <div class="product_info_content">
                                <%= product.desc %>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="sheet" role="tabpanel">
                            <div class="product_d_table">
                                <%- product.technical %>
                            </div>
                        </div>

                        <div class="tab-pane fade" id="reviews" role="tabpanel">

                            <!--! Comment -->
                            <%- include("./includeComment") %>
                                <!--! Comment -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--product info end-->

