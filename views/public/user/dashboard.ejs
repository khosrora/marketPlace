<%- include("./includeBreadCrumb")%>
    <!-- my account start  -->
    <section class="main_content_area">
        <div class="container">
            <div class="account_dashboard">
                <div class="row">
                    <div class="col-sm-12 col-md-3 col-lg-3">
                        <!-- Nav tabs -->
                        <%- include("./includeMenu")%>
                    </div>
                    <div class="col-sm-12 col-md-9 col-lg-9">
                        <!-- Tab panes -->
                        <div class="tab-content dashboard_content">
                            <div class="tab-pane fade show active" id="dashboard">
                                <h3>داشبورد </h3>
                                <%- include("../../includes/message") %>
                                    <p>نام کاربری : <%= user.firstName %>
                                            <%= user.lastName %>
                                    </p>
                                    <p>پست الکترونیک : <%= user.email %>
                                    </p>
                                    <p>شماره همراه : <%= user.mobile %>
                                    </p>

                                    <% if(!user.address){%>
                                        <p class="text-danger">لطفا آدرس را از بخش ویرایش حساب وارد کنید</p>
                                        <%}%>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- my account end   -->