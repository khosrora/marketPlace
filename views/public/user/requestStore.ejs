<%- include("./includeBreadCrumb")%>
    <!-- my account start  -->
    <section class="main_content_area">
        <div class="container">
            <div class="account_dashboard">
                <div class="row">
                    <div class="col-sm-12 col-md-3 col-lg-3">
                        <!-- Nav tabs -->
                        <%- include("./includeMenu")%>
                    </div>
                    <div class="col-sm-12 col-md-9 col-lg-9">
                        <!-- Tab panes -->
                        <div>
                            <h3>درخواست پنل فروشندگی </h3>
                            <%- include("../../includes/errors") %>
                                <div class="login">
                                    <div class="login_form_container">
                                        <div class="contact_message form">
                                            <form action="/user/requestStore" method="POST">
                                                <label>نام فروشگاه</label>
                                                <input type="text" name="storeName">
                                                <label>ایمیل فروشگاه</label>
                                                <input type="email" name="storeEmail" dir="ltr">
                                                <label>شماره تماس فروشگاه</label>
                                                <input type="text" name="storeMobile" dir="ltr">
                                                <div class="contact_textarea">
                                                    <label> آدرس فروشگاه</label>
                                                    <textarea placeholder="پیام *" name="storeAddress"
                                                        class="form-control2"></textarea>
                                                </div>
                                                <div class="contact_textarea">
                                                    <label> توضیحات فروشگاه</label>
                                                    <textarea placeholder="پیام *" name="storeDesc"
                                                        class="form-control2"></textarea>
                                                </div>
                                                <div class="contact_textarea">
                                                    <label> توضیحات فروشنده</label>
                                                    <textarea placeholder="پیام *" name="storeAdminDesc"
                                                        class="form-control2"></textarea>
                                                </div>
                                                <div class="save_button primary_btn default_button">
                                                    <button type="submit">ارسال درخواست</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- my account end   -->